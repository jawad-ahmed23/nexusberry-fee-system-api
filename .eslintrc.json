{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "linebreak-style": "off",
    "import/extensions": "off",
    "import/prefer-default-export": "off",
    "operator-linebreak": "off",
    "comma-dangle": "off",
    "object-curly-newline": "off",
    "func-names": "off",
    "prefer-arrow-callback": "off",
    "no-underscore-dangle": "off"
  }
}
